               },\n            },\n        };\n\n        this.fieldData = Parser(settings);\n        this.init();\n    }\n\n    init() {\n        this.initBackground();\n        this.initField();\n    }\n\n    initBackground() {\n        this.container = this.addChild(Rect(\n            { x: -100, w: 600, h: 600 },\n            { color: 0x0CBCBCB },\n        ));\n    }\n\n    initField() {\n        this.field   = this.container.addChild(new Field(this.fieldData, { w: 600, h: 600 }));\n        this.field.x = -100;\n    }\n\n    onResize() {\n\n    }\n\n    tick(delta) {\n        if (this.field.tick) this.field.tick(delta);\n    }\n}\n"},"lineCount":null}},"error":null,"hash":"7bde390f5b3378076aa32c4d31eadfad","cacheData":{"env":{}}}